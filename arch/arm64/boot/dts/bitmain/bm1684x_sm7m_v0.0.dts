#include "bm1684x.dtsi"
#include "bm1684x_mm_16g.dtsi"
#include "bm1684x_evb.dtsi"
#include <dt-bindings/input/input.h>

/ {
	/delete-node/ pka@58008000;
	/delete-node/ clp890@50018000;
	/delete-node/ tach@50029028;
	info {
		file-name = "bm1684x_sm7m_v0.0.dtb";
	};
};

&vpu_reserved {
			reg = <0x3 0x40000000 0x0 0x08000000>; // 128M, for VPU firmware
	no-map;
};

&ion_for_vpu {
			compatible = "vpu-region";
			reg = <0x3 0x48000000 0x0 0x80000000>; // 2G, for VPU
};

&ion_for_npu {
			compatible = "npu-region";
			reg = <0x1 0x51000000 0x0 0x80000000>; // 2GB
};

&ion_for_vpp {
			compatible = "vpp-region";
			reg = <0x4 0x00000000 0x1 0x00000000>; // 4G, for VPP/JPU
};
/ {
	gpioleds: gpio-leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm1_release &dbgi2c_release>;
		status = "okay";

		gpio5 {
			label = "GPIO5";
			gpios = <&port0a 5 GPIO_ACTIVE_HIGH>;
		};

		gpio6 {
			label = "GPIO6";
			gpios = <&port0a 6 GPIO_ACTIVE_HIGH>;
		};

		gpio7 {
			label = "GPIO7";
			gpios = <&port0a 7 GPIO_ACTIVE_HIGH>;
		};

		gpio29 {
			label = "GPIO29";
			// gpio29 and gpio30 get swapped in schematic
			gpios = <&port0a 30 GPIO_ACTIVE_HIGH>;
		};

		p01 {
			label = "RELAY0";
			gpios = <&port6416 1 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		p02 {
			label = "RELAY1";
			gpios = <&port6416 2 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		p03 {
			label = "WORK_LED";
			gpios = <&port6416 3 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		p04 {
			label = "DIY_LED";
			gpios = <&port6416 4 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		#if 0
		p06 {
			label = "PCIE_SEL";
			gpios = <&port6416 6 GPIO_ACTIVE_HIGH>;
		};

		p07 {
			label = "LTE_RST";
			gpios = <&port6416 7 GPIO_ACTIVE_HIGH>;
		};

		p10 {
			label = "LTE_PWR_EN";
			gpios = <&port6416 8 GPIO_ACTIVE_HIGH>;
		};
		#endif

		p12 {
			label = "SATA_EN";
			gpios = <&port6416 10 GPIO_ACTIVE_HIGH>;
		};

	};

	gpiokeys: gpio-keys {
		compatible = "gpio-keys-polled";
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		debounce-interval = <300>;
		poll-interval = <100>;

		#if 0
		p05 {
			label = "power";
			linux,code = <KEY_POWER>;
			gpios = <&port6416 5 GPIO_ACTIVE_HIGH>;
		};
		#endif
	};

	pwmfan: pwm-fan {
		pinctrl-names = "default";
		pinctrl-0 = <&pwm0_acquire>;
		compatible = "pwm-fan";
		pwms = <&pwm 0 40000>; // period_ns
		pwm-names = "pwm0";
		pwm_inuse = "pwm0";
		#cooling-cells = <2>;
		cooling-levels = <0 160 192 224 255>;  //total 255
	};

};

&ethernet0 {
	eth-sophgo-config {
		led = <0xae00>;
		clkout = <0x0e02>;
	};
};

&ethernet1 {
	eth-sophgo-config {
		led = <0xae00>;
		clkout = <0x0e02>;
	};
};

&i2c1 {
	/delete-node/ rtc@68;

	port6416: tca6416@6c {
		compatible = "ti,tca6416";
		reg = <0x6c>;
		gpio-controller;
		#gpio-cells = <2>;
	};
};

&i2c2 {
	rtc@68 {
		compatible = "dallas,ds1307";
		reg = <0x68>;
	};
};
&clock_init {
	clock-rates = \
		<2300000000>, <750000000>, <640000000>,

		<1000000000>, <1000000000>,
		<2000000000>, <50000000>,
		<100000000>, <100000000>,
		<125000000>, <50000000>,
		<25000000>, <125000000>,
		<50000000>, <25000000>,
		<500000000>, <25000000>,
		<1000000000>, <500000000>,
		<800000000>, <100000000>,
		<250000000>, <250000000>,
		<500000000>, <500000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<100000>, <100000>, <100000>,

		<1000000001>,
		<800000000>, <800000001>,
		<2300000001>, <575000000>;
		};
